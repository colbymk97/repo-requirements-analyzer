# Copy to .env and set your real key.
OPENAI_API_KEY=your_openai_api_key_here
# Optional: custom OpenAI-compatible endpoint (default is https://api.openai.com/v1)
# OPENAI_BASE_URL=https://your-provider.example.com/v1
# Optional: responses (default) or chat_completions
# OPENAI_API_MODE=responses

# Azure OpenAI option:
# AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
# AZURE_OPENAI_API_KEY=your_azure_openai_key
# AZURE_OPENAI_API_VERSION=2024-10-21
# Optional: default deployment name (or pass deployment name with MODEL=...)
# AZURE_OPENAI_DEPLOYMENT=your_deployment_name
